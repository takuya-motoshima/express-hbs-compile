    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.22",
    "builtin-modules": "^3.1.0",
    "jest": "^29.3.1",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.2",
    "typescript": "^5.2.2"
