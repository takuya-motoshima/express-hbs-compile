    "prebuild": "rimraf dist types",
    "test": "jest",
    "test:clear-cache": "jest --clearCache"
  },
  "_moduleAliases": {
    "~": "."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takuya-motoshima/express-hbs-compile.git"
